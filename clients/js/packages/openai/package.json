{
  "name": "@chromadb/openai",
  "version": "2.0.0",
  "description": "Openai embeddings for chroma",
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/cjs/openai.cjs",
  "module": "dist/openai.legacy-esm.js",
  "browser": "dist/openai.browser.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/openai.d.ts",
        "default": "./dist/openai.mjs"
      },
      "require": {
        "types": "./dist/cjs/openai.d.cts",
        "default": "./dist/cjs/openai.cjs"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "postbuild": "attw --pack .",
    "prettier": "prettier --write ."
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@chromadb/core": "^2.0.0"
  },
  "peerDependencies": {
    "openai": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "2.8.7",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.0.4"
  }
}
